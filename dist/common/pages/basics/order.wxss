.body>movable-area{background:#fff;width:100%}.body>movable-area movable-view{width:var(--w);height:var(--w)}.body>movable-area movable-view view{width:calc(100% - 4px);height:calc(100% - 4px);display:flex;align-items:center;justify-content:center;box-sizing:border-box;border:1px solid #ddd;margin:2px;border-radius:6rpx;font-size:27rpx;background:#fff}.body>movable-area view.add,.body>movable-area view.item-tem{width:calc(var(--w, 0) - 4px);height:calc(var(--w, 0) - 4px);display:flex;align-items:center;justify-content:center;box-sizing:border-box;border:1px solid #ddd;margin:2px;border-radius:6rpx;position:absolute;font-size:60rpx;color:#777;top:var(--top);left:var(--left);background:#fff}.body>movable-area view.item-tem{font-size:27rpx;transition:all var(--s)}